<%= simple_form_for [:admin, @classification] do |f| %>
    <%= f.error_notification %>
    <div class="form-inputs">
      <%= f.input :name %>
      <%= f.input :level %>
      <%= f.input :upid %>
      <%= f.input :state  %>
    </div>
    <div class="form-actions">
      <%= f.submit class: 'btn btn-primary' %>
      <%= link_to '返回', 'javascript:window.history.go(-1);', class: 'btn' %>
    </div>
<% end %>
